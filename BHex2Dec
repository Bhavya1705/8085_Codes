MVI D, 00H
MVI E, 00H
MVI H, 00H
IN 00H; Input from User
MVI L, 09H;
;MOV B,A;Initialising input value in B
MVI B, 2CH
MVI C, 000H
LOOP1: DCR B
JM LOOP3
INR D; Ones Value
JZ LOOP3
MOV A,D
CMP L
JZ LOOP19
JMP LOOP1
LOOP19: MOV A,B
ADD C
JZ LOOP3
JMP LOOP2
LOOP2: INR E; Tens Value
MVI D, 00H
DCR B
JM LOOP3
JZ LOOP3 
ADD L
JMP LOOP1
LOOP3: MVI C,010H
ADD C 
MOV A,E
ADD E
JZ LOOP4 
MOV A,H
ADD C
MOV H,A
DCR E
JM LOOP4
JZ LOOP4
JMP LOOP3
LOOP4: MOV A,H
ADD D
MOV H,A 
hlt
